# Google OAuth Credentials
# To use REAL Auth, replace these with valid keys from your provider.
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Facebook OAuth Credentials
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# Session Secret
SESSION_SECRET=frailty_tester_secret_key_change_this_in_production

# MOCK AUTH MODE
# Set to 'true' to simulate successful Google/Facebook login without real keys.
USE_MOCK_AUTH=true
